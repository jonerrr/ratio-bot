generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Ratio {
  id       String @id @map("_id")
  likes    Int    @default(1)
  userId   String
  serverId String @unique
  username String
  related  String @unique
}
