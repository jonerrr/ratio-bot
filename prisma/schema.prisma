generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Ratio {
  id       String @id @map("_id")
  likes    Int    @default(1)
  userId   String
  serverId String @unique
  username String
  expire   Int?
  related  String @unique
}

model User {
  id         String   @id @map("_id")
  voteExpire Int
  emojis     Emojis[]
}

enum Emojis {
  RED
  WHITE
  BLACK
  PURPLE
  BLUE
  GREEN
  YELLOW
  ORANGE
  BROWN
}
